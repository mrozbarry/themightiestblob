    "node-sass": "^3.1.2",
    "sinon": "^1.16.1",
    "webpack-dev-server": "^1.7.0",
    "webworker-threads": "^0.5.7"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --opts spec/mocha.opts spec",
